#include <iostream>
using namespace std;

int main() {
    int n, arr[205];
    cout << "Masukkan jumlah peserta lomba lari: ";
    cin >> n;

    cout << "Masukkan waktu tempuh lari tiap peserta (dalam detik):\n";
    for (int i = 0; i < n; i++) cin >> arr[i];

    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j+1]) {
                swap(arr[j], arr[j+1]);
            }
        }
    }

    cout << "Waktu tempuh peserta setelah diurutkan (dari tercepat ke terlambat):\n";
    for (int i = 0; i < n; i++) cout << arr[i] << " ";
    return 0;
}
